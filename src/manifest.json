{
	"name": "__MSG_extn_name__",
	"version": "@version@",/*!debug*/
	"version": "0.0.0",/*gubed!*/
	"default_locale": "en_GB",
	"description": "__MSG_extn_description__",
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"permissions": [
		"http://*/*",
		"https://*/*",
		"tabs"
	],
	"content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
	"content_scripts": [{
		"matches": ["*://*/*"],
		"js": ["validity.js"]
	}],
	"background": {
		"scripts": [
			"background/controller.js",
			"background/net.js",
			"background/stats.js",
			"background/xml.js",
			"background/ui.js",
			"background/util.js",
			"background/opts.js"
		]
	},
	"page_action": {
		"default_icon": "img/html_valid.png",
		"default_title": "__MSG_page_action_title__"
	},
	"options_page": "options.html",
	"manifest_version": 2
}
